{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@es/*": ["src/components/es/*"],
      "@en/*": ["src/components/en/*"],
      "@js/*": ["src/js/*"],
      "@content/*": ["content/*"]
    },
    "jsx": "preserve"
  }
}
